{
    "name": "carsdotcom/php-request-class",
    "type": "project",
    "version": "v1.0.0",
    "description": "Structure the logic around Guzzle API requests into object-oriented classes",
    "keywords": ["php", "laravel", "guzzle"],
    "license": "MIT",
    "require": {
        "carsdotcom/laravel-json-schema": "^v1.0",
        "ext-simplexml": "*",
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.19"
    },
    "require-dev": {
        "brianium/paratest": "^6.6",
        "fakerphp/faker": "^1.9.1",
        "nunomaduro/collision": "^6.1",
        "orchestra/testbench": "^7.11",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.7",
        "timacdonald/log-fake": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Carsdotcom\\ApiRequest\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carsdotcom\\ApiRequest\\ApiRequestProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
